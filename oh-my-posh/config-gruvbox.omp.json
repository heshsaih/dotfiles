{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#d65d0e",
          "style": "plain",
          "template": "\u279c",
          "type": "text"
        },
        {
          "foreground": "#b8bb26",
          "properties": {
            "style": "folder"
          },
          "style": "plain",
          "template": " {{ .Path }}",
          "type": "path"
        },
        {
          "foreground": "#fb4934",
          "properties": {
            "branch_icon": ""
          },
          "style": "plain",
          "template": " <#83a598>git:(</>{{ .HEAD }}<#83a598>)</>",
          "type": "git"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 3,
  "properties": {
    "fetch_status": false
  }
}
