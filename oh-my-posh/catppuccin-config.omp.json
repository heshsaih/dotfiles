{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#f9e2af",
          "style": "plain",
          "template": "\u279c",
          "type": "text"
        },
        {
          "foreground": "#cba6f7",
          "properties": {
            "style": "folder"
          },
          "style": "plain",
          "template": " {{ .Path }}",
          "type": "path"
        },
        {
          "foreground": "#eba0ac",
          "properties": {
            "branch_icon": ""
          },
          "style": "plain",
          "template": " <#89b4fa>git:(</>{{ .HEAD }}<#89b4fa>)</>",
          "type": "git"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 3,
  "properties": {
    "fetch_status": false
  }
}
